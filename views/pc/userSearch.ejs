                    info += row.season + ", " + row.nickname + ", ì²´í¬ì¼:" + row.checkingDate + "\n";
                }

                alert(info);
            }

            $(function () {
                const $dropZone = $('#drop-zone');
                const $result = $('#result');

                // ë“œë˜ê·¸ ì˜¤ë²„
                $dropZone.on('dragover', function (e) {
                    e.preventDefault();
                    e.stopPropagation();
                    $(this).addClass('dragover');
                });

                // ë“œë˜ê·¸ ë¦¬ë¸Œ
                $dropZone.on('dragleave', function (e) {
                    e.preventDefault();
                    e.stopPropagation();
                    $(this).removeClass('dragover');
                });

                // ë“œë¡­
                $dropZone.on('drop', function (e) {
                    e.preventDefault();
                    e.stopPropagation();
                    $(this).removeClass('dragover');

                    const file = e.originalEvent.dataTransfer.files[0];
                    if (!file) return;

                    // ë¯¸ë¦¬ë³´ê¸° ë˜ëŠ” ì—…ë¡œë“œ ì²˜ë¦¬
                    uploadImage(file);
                });

                function uploadImage(file) {
                const formData = new FormData();
                formData.append('image', file);

                $result.text('ì—…ë¡œë“œ ì¤‘...');

                $.ajax({
                    url: '/user/getOcrNames',
                    type: 'POST',
                    data: formData,
                    processData: false,
                    contentType: false,
                    success: function (data) {
                        inferSearch(data.teamA.join(" "), 'con1_2');
                        inferSearch(data.teamB.join(" "), 'con2_2');
                    },
                    error: function (xhr, status, err) {
                        console.error(err);
                        $result.text('ì—…ë¡œë“œ ì‹¤íŒ¨ ğŸ˜¥');
                    }
                });
                }
            });

            const inferSearch = (nicknames, divId)  => {
                if (nicknames && nicknames != "") {
                    $("#nickNames").val(nicknames);
                    search('nickNames', divId);
                }
            }

